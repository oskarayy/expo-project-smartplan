{
  "expo": {
    "name": "expo-project-smartnotes",
    "slug": "expo-project-smartnotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon-multi/ic_launcher.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-bg-md.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/app-icon-multi/ic_launcher.png",
          "color": "#000000"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon-multi/play_store_512.png",
        "backgroundColor": "#000000"
      },
      "package": "com.oskarayy.expoprojectsmartnotes"
    },
    "web": {
      "favicon": "./assets/app-icon-multi/ic_launcher.png"
    },
    "extra": {
      "eas": {
        "projectId": "958a8050-8123-4c28-821e-b537343ef38c"
      }
    }
  }
}
